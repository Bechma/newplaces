{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "prettier",
    "html",
    "@typescript-eslint"
  ],
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-non-null-assertion": 0
  }
}
